{
    "name": "decodelabs/hydro",
    "description": "Simple PSR18 HTTP client wrapper around Guzzle",
    "type": "library",
    "keywords": [ ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",

        "decodelabs/atlas": "^0.14.1",
        "decodelabs/coercion": "^0.3.5",
        "decodelabs/exceptional": "^0.6.3",
        "decodelabs/kingdom": "^0.1",

        "psr/http-client": "^1.0.3",
        "psr/http-message": "^2.0",
        "guzzlehttp/guzzle": "^7.9.3"
    },
    "require-dev": {
        "decodelabs/collections": "^0.12",
        "decodelabs/phpstan-decodelabs": "^0.7"
    },
    "conflict": {
        "decodelabs/collections": "<0.12"
    },
    "suggest": {
        "decodelabs/collections": "Fetch JSON files over HTTP as Tree"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Hydro\\": "src/Hydro/"
        },
        "classmap": [
            "src/Hydro.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "DecodeLabs\\Hydro\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.2.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
